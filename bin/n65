#!/usr/bin/env ruby
# frozen_string_literal: true

###############################################################################
##  6502 Assembler for the NES's 2A03
##
##  Usage: ./n65 <infile.asm>
##
##  This file runs the assembler though the commandline frontend.

require_relative '../lib/n65/front_end'

N65::FrontEnd.new(ARGV).run
